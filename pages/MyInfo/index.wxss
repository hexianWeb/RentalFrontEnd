.container {
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  z-index: -1;
  overflow: hidden;
}
.container .userinfo {
  margin-top: 7rpx;
}
.container .userinfo::after {
  content: "";
  width: 140%;
  height: 100%;
  position: absolute;
  border-radius: 0 0 50% 50%;
  left: -20%;
  top: -20%;
  z-index: -1;
  background-image: linear-gradient(135deg, #e96d71 10%, #fa742b 100%);
}
.master_info {
  width: 100%;
  margin-top: 0rpx;
}
.master_info van-cell {
  width: 100% !important;
  color: black !important;
}
